@Library('CICD library')_

pipeline
{
    agent any
    stages
    {
        stage('continuousdownload')
        {
            steps
            {
                script
                {
                    cicd.gitDownload('mavenrepo')
                }
            }
        }
        stage('continuousbuild')
        {
            steps
            {
                script
                {
                    cicd.gitbuild()
                }
            }
        }
 
    }
}
